<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/main.css" />
    <title>  </title>
  </head>
  <body>
    <header class="header">
      <h2> Shop your gown</h2>
      </header>
    <main class="main" > 
      
      
    <% dressesList.forEach( (item) =>{ %>
      <section class="container">
    
    <h4><%= item.nameDress %></h4>
    <img src="<%= item.image %>" alt="img">
    <p> <%= item.style %> </p>
    <p> <%= item.designer %> </p>
    <p> <%= item.yearCollection %> </p>
   </section>
    <% }) %>
  
</main>
<footer>
</footer>
  </body>
</html>